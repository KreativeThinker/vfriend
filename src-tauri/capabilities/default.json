{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "store:default",
    "os:default",
    "deep-link:default",
    "opener:default",
    "barcode-scanner:default",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "**"
        }
      ]
    }
  ]
}
